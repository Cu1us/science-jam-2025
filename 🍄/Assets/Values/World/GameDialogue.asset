  gameDialogue:
  - dialogueBoxes:
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CThis fungus strives for survival\u201D"
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus:\u201CIt stumbled upon human, laying in cold snow, no
        good environment for human.\u201D"
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CDo human strive to survive?\u201D"
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: 'Bob Ek: "I do"'
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CFungus will give warmth if fed nutrients, such
        could be found in bushes. This land is barren but one is close by\""
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \"If you let Fungus live on human, both will not die.
        Does this deal seem advantageous to the perspective of human?\u201D"
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: 'Bob Ek: "It does"'
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CHurray! Now let's grub on foliage!\u201D"
  - dialogueBoxes:
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CThis fungus consumes elements, such humans don't
        yet know of, in the ground by this bush. "
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CIt makes the bush prosper. In return fungus borrows
        nutrients from bush.\u201D  "
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: "Bob Ek: \u201CBeneficial!\u201D"
  - dialogueBoxes:
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CThis Human is getting cold, let's return to camp.
        Fungus will grow with rest.\u201D"
  - dialogueBoxes:
    - characterPortrait: {fileID: 0}
      dialogueText: 
  - dialogueBoxes:
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CDid human sleep well?\u201D"
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: "Bob Ek: \u201CLike a rock\u201D"
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CRocks don't sleep\u201D"
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: "Bob Ek: \u201CDo you sleep?\u201D"
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CNo, fungus does not understand it.\u201D"
  - dialogueBoxes:
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: "Bob Ek: \u201CHow can I talk with you?\u201D "
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CHuman can't, only \u2018think with\u2019\u201D"
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CThis fungus mycelium connected to humans brain\u201D"
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CNo, fungus does not understand it.\u201D"
  - dialogueBoxes:
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CWhy do human wish to survive?\u201D"
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: "Bob Ek: \u201CI need to get in contact with someone, let them
        know I'm alive\""
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: "Bob Ek: \u201CWhy do you want to survive?\u201D"
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Bob Ek: \u201CThere's a radio tower on top of this mountain\u201D"
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: "Bob Ek: \u201CWhy do you want to survive?\u201D"
    - characterPortrait: {fileID: 21300000, guid: 399bb3c6394a5864e8b56f0f7620faf1,
        type: 3}
      dialogueText: "Fungus: \u201CNo reason, It's what fungus do.\u201D"
    - characterPortrait: {fileID: 21300000, guid: d833553c48f638f4abc37b65716af80d,
        type: 3}
      dialogueText: "Bob Ek: \u201COdd to have no reason.\u201D"
